<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox prefWidth="230.0" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <VBox prefWidth="230.0" spacing="10.0">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="10.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="23.0" prefWidth="182.0" spacing="10.0">
                     <children>
                        <Label id="comment_Writer" text="글쓴이" />
                        <Label id="comment_No" text="번호" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="23.0" prefWidth="154.0" spacing="5.0">
                     <children>
                        <!-- <Button id="comment_RewriteBtn" fx:id="comment_Rewrite" mnemonicParsing="false" text="수정" /> -->
                        <Button id="comment_DeleteBtn" fx:id="comment_Delete" mnemonicParsing="false" text="삭제" />
                     </children>
                  </HBox>
               </children>
            </HBox>
            <Label id="comment_Content" text="내용" />
            <HBox alignment="CENTER_LEFT" prefWidth="200.0">
               <children>
                  <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../images/time.jpg" />
                     </image>
                  </ImageView>
                  <Label id="comment_Time" text="시간" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</HBox>
